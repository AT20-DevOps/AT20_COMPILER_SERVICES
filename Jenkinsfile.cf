pipeline {
    agent any
    stages {
        stage('Test') {
            agent {
                docker {
                    image 'node:18-alpine3.16'
                }
            }
            steps {
                sh 'npm install'
                sh 'npm install --save-dev babel-jest @babel/core @babel/preset-env'
                sh 'npm install --save-dev @babel/preset-typescript'
                sh 'npm run test'
            }
        }
    }
}